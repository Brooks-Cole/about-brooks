# Example .env file - rename to .env and update with your actual values
# This is the Anthropic API key format (example only, not a real key)
ANTHROPIC_API_KEY=sk-ant-api03-XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX-XXXXXXX

# Required for session security
SECRET_KEY=your_secret_key

# AWS S3 Configuration for image storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=your-s3-bucket-name
AWS_REGION=us-east-1

# Optional: other environment variables you might want to set
# DEBUG=True
# ENVIRONMENT=development